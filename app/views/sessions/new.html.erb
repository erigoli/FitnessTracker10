<h1>Login</h1>

<%= form_with(url: login_path, method: :post) do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username %><br>

  <%= form.label :password %>
  <%= form.password_field :password %><br>

  <%= form.submit "Log In" %>
<% end %>

<%= link_to "Sign Up", new_user_path %>
